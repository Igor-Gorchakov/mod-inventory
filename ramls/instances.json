{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "instances": {
      "id": "instances",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "@context": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "instanceTypeId": {
            "type": "string"
          },
          "source": {
            "type": "string"
          },
          "identifiers": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "object",
              "properties": {
                "identifierTypeId": {
                  "type": "string"
                },
                "value": {
                  "type": "string"
                }
              },
              "required": [
                "identifierTypeId",
                "value"
              ]
            }
          },
          "links": {
            "type": "object",
            "properties": {
              "self": {
                "type": "string"
              }
            },
            "required": [
              "self"
            ]
          }
        },
        "required": [
          "title",
          "source",
          "instanceTypeId"
        ]
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "required": [
    "instances",
    "totalRecords"
  ]
}
